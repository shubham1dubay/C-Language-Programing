#include<stdio.h>
int add(int arr[],int n);
int main()
{
	int arr1[10]={2,4,6,8,10,12,14,25,33,12};
	int arr2[8]={1,2,3,4,5,6,7,8};
	int arr3[6]={1,2,3,4,5,6};
	printf("Sum of element of arry %d: \n",add(arr1,10));
	printf("Sum of element of arry %d: \n",add(arr2,8));
	printf("Sum of element of arry %d: \n",add(arr3,6));
	return 0;
}
int add(int arr[],int n)
{
	int i,sum=0;
	for(i=0;i<n;i++)
	sum+=arr[i];
	return sum;
}
